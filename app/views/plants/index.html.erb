<div class="content">
  <div class="container">
    <h2> Plants available around you</h2>
    <div class="row">
      <% @plants.each do |plant| %>
      <div class="col-xs-12 col-sm-4">
        <%= render "shared/card", plant: plant %>
      </div>
    <% end %>
    </div>
  </div>
  <div id="map" style="width: 100%; height: 600px;" data-markers="<%= @markers.to_json %>"></div>
</div>

