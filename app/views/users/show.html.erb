  <div class="profile-dash text-center">
    <div class="main-dash">
      <div class="main-content">
        <%= image_tag "https://kitt.lewagon.com/placeholder/users/Alexialet", class: "img-circle" %>
        <h2>User's name</h2>
        <p><%= @user.email %></p>
      </div>
    </div>
      <div class="user-plant">
        <h3>All my plants</h3>
          <% @user.plants.each do |plant| %>
          <div class="col-xs-6 col-sm-4">
          <p><%= plant.name %></p>
          <p><%= plant.photo %></p>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>


